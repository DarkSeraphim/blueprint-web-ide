{
  "extends": "@snowpack/app-scripts-svelte",
  "mount": {
    "public": "/",
    "src": "/blueprint-web-ide/_dist_"
  },
  "plugins": [
    ["@snowpack/plugin-webpack"]
  ],
  "installOptions": {
    "installTypes": true,
    "treeshake": true
  },
  "alias": {
    "@ide": "./src"
  },
  "buildOptions": {
    "baseUrl": "/blueprint-web-ide",
    "sourceMaps": true
  }
}